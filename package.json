{
  "name": "permissions-service-koliko",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "clean": "rm -rf dist",
    "setup": "npm run build && npm run setup:db && npm run setup:kv",
    "setup:db": "psql -h localhost -U postgres -d permissions_db -f sql/init.sql",
    "setup:kv": "nats kv add permissions_cache",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["nats", "permissions", "microservice", "rpc"],
  "author": "",
  "license": "ISC",
  "description": "Permissions microservice using NATS RPC and PostgreSQL",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/pg": "^8.15.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "nats": "^2.29.3",
    "pg": "^8.16.2"
  },
  "type": "commonjs"
}